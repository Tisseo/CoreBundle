{% extends "::base.html.twig" %}

{% block stylesheets %}
    {{ parent() }}
    {% stylesheets 'bundles/tisseocore/css/main.css'
                   filter='cssrewrite' output='css/tisseo_core.css' %}
        <link rel="stylesheet" href="{{ asset_url }}?{{ assets_version }}" />
    {% endstylesheets %}
    {% stylesheets 'bundles/bootstrapcolorpicker/css/bootstrap-colorpicker.min.css'
            filter='cssrewrite' %}
         <link rel="stylesheet" href="{{ asset_url }}?{{ assets_version }}" />
    {% endstylesheets %}
    {% stylesheets 'bundles/tisseocore/css/select2.min.css'
        filter='cssrewrite' output='css/select2.min.css' %}
        <link rel="stylesheet" href="{{ asset_url }}?{{ assets_version }}" />
    {% endstylesheets %}
    {% stylesheets 'bundles/tisseocore/css/select2-bootstrap.css'
        filter='cssrewrite' output='css/select2-bootstrap.min.css' %}
        <link rel="stylesheet" href="{{ asset_url }}?{{ assets_version }}" />
    {% endstylesheets %}
{% endblock %}

{% block head_config %}
    {{ parent() }}
    {% include 'TisseoCoreBundle::config.js.html.twig' %}
{% endblock %}

{% block navbar_brand %}{% endblock %}
{% block root_breadcrumb %}{% endblock %}

{% block body %}
    {% block subheader %}
    {% endblock %}
    {% block container %}
    {% endblock %}
{% endblock %}
